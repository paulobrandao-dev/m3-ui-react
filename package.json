{
  "name": "@paulobrandao/react-material",
  "version": "1.0.0",
  "type": "module",
  "license": "ISC",
  "homepage": "https://github.com/paulobrandao-dev/react-material",
  "author": {
    "name": "Paulo Brand√£o",
    "url": "https://paulobrandao.dev",
    "email": "paulobrandao.dev@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulobrandao-dev/react-material.git"
  },
  "files": [
    "dist",
    "badges.svg"
  ],
  "main": "./dist/react-material.cjs",
  "module": "./dist/react-material.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-material.js",
      "require": "./dist/react-material.cjs"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/react-material.theme.js",
      "require": "./dist/react-material.theme.cjs"
    },
    "./icon": {
      "types": "./dist/icon.d.ts",
      "import": "./dist/react-material.icon.js",
      "require": "./dist/react-material.icon.cjs"
    },
    "./dist/react-material.css": {
      "import": "./dist/react-material.css",
      "require": "./dist/react-material.css"
    },
    "./dist/react-material.icon-outlined.css": {
      "import": "./dist/react-material.icon-outlined.css",
      "require": "./dist/react-material.icon-outlined.css"
    },
    "./dist/react-material.icon-rounded.css": {
      "import": "./dist/react-material.icon-rounded.css",
      "require": "./dist/react-material.icon-rounded.css"
    },
    "./dist/react-material.icon-sharp.css": {
      "import": "./dist/react-material.icon-sharp.css",
      "require": "./dist/react-material.icon-sharp.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build:docs": "tsc -b && vite build -c configs/vite.config.docs.ts",
    "build:outlined": "tsc -b && vite build -c configs/vite.config.icon-outlined.ts",
    "build:rounded": "tsc -b && vite build -c configs/vite.config.icon-rounded.ts",
    "build:sharp": "tsc -b && vite build -c configs/vite.config.icon-sharp.ts",
    "build:icons": "npm run build:outlined && npm run build:rounded && npm run build:sharp",
    "build": "tsc -b && vite build && npm run build:icons",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --coverage.enabled=true --ui",
    "coverage": "vitest run --coverage",
    "make-badges": "coverage-badges --output ./badges.svg",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write --config .prettierrc.json"
    ],
    "*.{scss,css}": "prettier --write --config .prettierrc.json"
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "clsx": "^2.1.1",
    "material-symbols": "^0.33.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@happy-dom/global-registrator": "^18.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "coverage-badges-cli": "^2.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "rollup-preserve-directives": "^1.1.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.2.4"
  }
}
